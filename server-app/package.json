{
  "name": "tangoregister-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "build": "rimraf dist && esbuild src/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && esbuild src/vercel-handler.ts --platform=node --bundle --format=cjs --outfile=dist/vercel-handler.cjs",
    "vercel-build": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push --config ../drizzle.config.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "stripe": "^18.4.0",
    "vite": "^5.4.19",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.16.11",
    "@types/qrcode": "^1.5.5",
    "@vercel/node": "^3.0.7",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  }
}

